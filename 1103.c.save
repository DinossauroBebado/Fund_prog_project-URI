#include <stdio.h>

int main() {
 int h1,m1,h2,m2,M;
    while (1) {
    scanf("%d %d %d %d", &h1, &m1, &h2, &m2);
    if ((h1 == 0) && (m1 == 0) && (h2 == 0) && (m2 == 0)){
    break;
    }
    if(h2>h1){
        //subtração simples
       if(m2>m1){
            M = (60*(h2-h1) + (m2-m1));
       }
       if(m1>m2){
            M = (60*(h2-h1) + (m1+m2-60));
       }
    }if(h1>h2){
       if(m2>m1){
            M = (60*(h1+24-h2) + (m2-m1));
       }
       if(m1>m2){
            M = (60*(h1+24-h2) + (m1+m2-60));
       }
    }if(h1==h2){
       if(m2>m1){
            M =(m2-m1);
       }
       if(m1>m2){
            M = (24*60 - (m1+60+m2));
        }
    }
    printf("%d\n",M);

}

    return 0;
}
